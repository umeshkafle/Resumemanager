<%= render 'pagenav'%>
  <div class="table-responsive">
    <%= render 'tableheader'%>
      <table class="table table-striped table-bordered table-hover">
        <thead>
          <tr>
            <th class="col-md-1">S.N</th>
            <th>Applicant Email</th>
            <th>Intervies Date</th>
            <th>Interview Time</th>
            <th>Attachment</th>
            <th>Scheduling</th>
            <th>Summary</th>
          </tr>
        </thead>
        <tbody>
      
          <% @schedules.each do |schedule| %>
              <tr>
                <td class="col-md-1" ><%= @schedules.index(schedule) + 1%></td>
                <td class="col-md-2"><%= schedule.from %></td>
                <td class="col-md-2"><%= schedule.interview_time.localtime %></td>
                <td class="col-md-1"><%= schedule.attachment %></td>
                <td class="col-md-2"></td>
                 
                 
                <td class="col-md-2">
                
                  <%= link_to 'ReSchedule Interview', "new_curriculumvitae_schedule_path(curriculumvitae.id)", class: "btn btn-info", remote: true %></td>
                <td class="col-md-2">
                  <%= link_to "Add", "new_curriculumvitae_summary_path(@curriculumvitae)", class: "btn btn default"%><span class="glyphicon glyphicon-plus"></span>
                  <%= link_to "view", "curriculumvitae_summaries_path(@curriculumvitae)", class: "btn btn default"%><span class="glyphicon glyphicon-plus"></span>


                  </td>
              </tr>
            
          <%end%>
    
        </tbody>
      </table>
  </div>
  <div id="pop-up-modal">
  </div>
  




