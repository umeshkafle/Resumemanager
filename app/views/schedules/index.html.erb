<%= render 'pagenav'%>	

	<div class="table-responsive">
    <%= render 'tableheader'%>
	    <table class="table table-striped table-bordered table-hover">
	      <thead>
	        <tr>
	          <th class="col-md-1">S.N</th>
	          <th>Applicant Email</th>
	          <th>Intervies Date</th>
	          <th>Interview Time</th>
	          <th>Attachment</th>
	          <th>Scheduling</th>
	          <th>Summary</th>
	        </tr>
	      </thead>
	      <tbody>
	      <% @schedules.each do |schedule|%>
	      	<tr>
	      	
	          <td class="col-md-1"><%= @schedules.index(schedule) + 1%></td>
	          <td class="col-md-2"><%= schedule.from %></td>
	          <td class="col-md-2"><%= schedule.interview_date %></td>
	          <td class="col-md-1.5"><%= schedule.interview_time %></td>
	          <td class="col-md-2"><%= schedule.attachment %></td>
	          <td class="col-md-2"> <%= link_to 'ReSchedule Interview', new_curriculumvitae_schedule_path(schedule.curriculumvitae), class: "btn btn-info" %>
	          	<%= link_to "View Details",""%>
	          </td>
	          <td class="col-md-2"><%= link_to "Add", "#", class: "btn btn-info"%>  <span class="glyphicon glyphicon-plus"></span>
	          	<%= link_to "View", "#", class: "btn btn-success"%>  <span class="glyphicon glyphicon-eye-open"></td>
	        </tr>
	      <%end%>
	      </tbody>
	    </table>
  </div>
  <div id="pop-up-modal">
	</div>

