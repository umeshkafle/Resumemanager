
<div class="container">
  <nav id="table-tabs">
      <ul class="nav nav-tabs">
        <li><%= link_to "CV Lists", root_url%></li>
        <li><%= link_to "Shorted CVs", "#"%></li>
        <li><%= link_to "Interview Schedule", "#"%></li>
      </ul>
      
  </nav>
  <div class="table-responsive">
    <div id="cv-header">
      <h2>CVs Lists</h2> 
      <form class="navbar-form navbar-right" role="search">
                <div class="form-group">
                  <input type="text" class="form-control" placeholder="Search">
                </div>
                <button type="submit" class="btn btn-default">Submit</button>
      </form> 
    </div>
    <table class="table table-striped table-bordered table-hover">
      <thead>
        <tr>
          <th class="col-md-1">S.N</th>
          <th>Send_By</th>
          <th>Received_on</th>
          <th>Attachment</th>
          <th>Actions</th>
        </tr>
      </thead>
      <tbody>
        
          <tr>
           <td class="col-md-2" ></td>
           <td class="col-md-2"></td>
           <td class="col-md-2"></td>
           <td class="col-md-2"></td>
           
           
           <td class="col-md-2">
              <%= link_to 'Schedule Interview', "#", class: "btn btn-info" %>
              
           </td>
          </tr>
        
      </tbody>
    </table>
  </div>
</div>

